pragma solidity 0.7.1;


contract WeddingGift {
    string public receiver = "Zhenya i Bazhenya";
    string public from = "Igor Sobolev";
    string public date = "September 27, 2020";

    // Look for seed phrase in the gift
    address internal immutable withdrawer;

    constructor(address payable withdrawer_) payable {
        withdrawer = withdrawer_;
    }

    function withdrawGift(address payable to) public {
        require(msg.sender == withdrawer, "Only withdrawer can do this!");

        to.transfer(address(this).balance);
    }
}
