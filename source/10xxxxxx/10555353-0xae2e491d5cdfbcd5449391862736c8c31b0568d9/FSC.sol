    function getLevel(uint value, uint _type) public view returns (uint) {
        if (value >= 1 * ethWei && value <= 3 * ethWei) return 1;
        if (value >= 4 * ethWei && value <= 6 * ethWei) return 2;
        if (_type == 1 && value >= 7 * ethWei) return 3;
