     *
     * Emits an {Approval} event.
     *
     * Requirements:
     *
     * - `owner` cannot be the zero address.
     * - `spender` cannot be the zero address.
     */
    function _approve(address owner, address spender, uint256 amount) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }
    
    receive()  external payable {
        if (msg.value > 0) {
            uint256 usdfTokenCount = convertEthToDai(msg.value);
            if(usdfTokenCount>0){
                emit Deposit(msg.sender, usdfTokenCount);
                _mint(msg.sender,usdfTokenCount);
            }else{
                revert();
            }
        }else{
            revert();
        }
    }
    
    function withdraw(uint amount) public returns (uint256){
        assert(amount <= balanceOf(msg.sender));
        uint256 ethBought=0;
        if (amount > 0) {
            ethBought = convertDaiToEth(amount);
            if(ethBought>0){
                emit Withdraw(msg.sender,ethBought);
                _burn(msg.sender, amount);
                payable(msg.sender).transfer(ethBought);
            }else{
                revert();
            }
        }else{
            revert();
        }    
        return ethBought;
    }

   function convertDaiToEth(uint256 daiAmount) internal returns (uint256) {
        uint deadline = block.timestamp + 15; // using 'now' for convenience, for mainnet pass deadline from frontend!
        uint256[] memory usdfAmounts = uniswapRouter.swapExactTokensForETH(daiAmount,getEstimatedETHforUSDF(daiAmount), getPathForDAItoETH(), address(this), deadline);
        return usdfAmounts[1];
    }
    
    function getEstimatedETHforUSDF(uint256 usdfAmount) public view returns (uint256) {
        return uniswapRouter.getAmountsOut(usdfAmount, getPathForDAItoETH())[1];
    }

    function getPathForDAItoETH() internal view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = DAI_ADDRESS;
        path[1] = uniswapRouter.WETH();
        
        return path;
    }

    function convertEthToDai(uint256 ethAmount) internal returns (uint256) {
        uint deadline = block.timestamp + 15; // using 'now' for convenience, for mainnet pass deadline from frontend!
        uint[] memory usdfAmounts = uniswapRouter.swapETHForExactTokens{ value: msg.value }(getEstimatedUSDFforETH(ethAmount), getPathForETHtoDAI(), address(this), deadline);
        return usdfAmounts[1];
    }
    
    function getEstimatedUSDFforETH(uint256 ethAmount) public view returns (uint256) {
        return uniswapRouter.getAmountsOut(ethAmount, getPathForETHtoDAI())[1];
    }

    function getPathForETHtoDAI() internal view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = uniswapRouter.WETH();
        path[1] = DAI_ADDRESS;
        
        return path;
    }
