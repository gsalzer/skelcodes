pragma solidity ^0.7.3;
pragma experimental ABIEncoderV2;

import '@openzeppelin/contracts/token/ERC721/ERC721.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';

contract ForgottenRunesDecoderCrystal is ERC721, Ownable {
    using SafeMath for uint256;
    uint256 public constant MAX_CRYSTALS = 1;

    constructor(string memory baseURI)
        ERC721('ForgottenRunesDecoderCrystal', 'CRYSTAL')
    {
        setBaseURI(baseURI);
    }

    function mint(bytes calldata s) public onlyOwner {
        require(totalSupply() < MAX_CRYSTALS, 'There can only be one');
        _safeMint(msg.sender, 0);
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        _setBaseURI(_newBaseURI);
    }
}

