    function getBall(uint256 tokenId) internal view returns (MuniLayerData memory) {
        uint256 rand = random(string(abi.encodePacked("Ball", toString(tokenId), _seed)));
        uint256 greatness = rand % 1000;
        if (greatness < 100) {
            return distanceBall;
        }
        if (greatness > 990) {
            return inHoleBall;
        }
        return groundBall;
    }

