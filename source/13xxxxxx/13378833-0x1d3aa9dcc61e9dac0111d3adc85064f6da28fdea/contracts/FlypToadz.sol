// SPDX-License-Identifier: MIT
pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract Flyptoadz is ERC721, Ownable {

    uint256 public constant maxTokens = 6969;
    uint256 public numAvailableTokens = 6969;
    uint256 public totalSupply = 0;

    IERC721 private sameToadz;
    string private baseURI;

    constructor(IERC721 _sameToadz) ERC721("FlypToadz", "FLOADZ") {
        sameToadz = _sameToadz;
    }
 
    function rescueToad(uint256 _id) public payable {
        // check sametoad ownership
        require(sameToadz.ownerOf(_id) == msg.sender, "Not able to free flyptoad.");

        _safeMint(msg.sender, _id);
        totalSupply += 1;
        numAvailableTokens -= 1;
    }

    function setBaseURI(string memory _URI) 
        external 
        onlyOwner 
    {
        baseURI = _URI;
    }
    
    function _baseURI() internal override(ERC721) view returns (string memory) {
        return baseURI;
    }

    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }
}
