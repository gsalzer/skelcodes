bool constant LEVEL_TRIGGER = false;
// false means EDGE_TRIGGER on price
interface KeeperCompatibleInterface {
    function checkUpkeep(bytes calldata checkData) external view
	returns (bool upkeepNeeded, bytes memory performData);
    function performUpkeep(bytes calldata performData) external;
}
contract KeeperPositions is
KeeperCompatibleInterface, IPositionLifecycle {
    function cancelPos(uint256 _orderId) pure external override {
	++_orderId;
    }
    function   fillPos(uint256 _orderId) pure external override {
	++_orderId;
    }
    function   openPos(uint256 _orderId) pure external override {
	++_orderId;
    }
    uint public count = 0;
    uint public interval = 0;
    uint public lastTimestamp = block.timestamp;
    bool public active = false;
    PositionMgr public positionMgr;
    function setPositionMgr(PositionMgr newPositionMgr) external {
	positionMgr = newPositionMgr;
	active = false;
    }
    function activate() external {
	active = true;
    }
    function deactivate() external {
	active = false;
    }
    function setInterval(uint updateInterval) external {
	interval = updateInterval;
    }
    function getPrice(IUniswapV3Pool pool) view public virtual returns (int24) {
	return positionMgr.tick(pool);
    }
    
    bool public direction = true;
    function toggleDirection() public {
	direction = !direction;
    }
    function triggered(uint256 id) view public returns (bool) {
	(IUniswapV3Pool pool,	
	 uint256 initialAmount0,
	 uint256 initialAmount1,
	 /*address user*/,
	 /*uint256 timeCreation*/,	
	 /*int24 tickCreation*/,
	 int24 tickLower,
	 int24 tickUpper,
	 /*uint128 liquidity*/,
	 /*PositionMgr.OrderType orderType*/,
	 PositionMgr.OrderState orderState) = positionMgr.order(id);
	if(orderState < PositionMgr.OrderState.OPEN)
	    return false;
	int24 tick = getPrice(pool);
	if(direction){
	    if (initialAmount0 > 0)
		return tick > tickUpper;
	    if (initialAmount1 > 0)
		return tick < tickLower;
	}else{
	    if (initialAmount0 > 0)
		return tick < tickLower;
	    if (initialAmount1 > 0)
		return tick > tickUpper;
	}
	return false;
    }
    function checkUp() view public returns (uint256[] memory _ids) {
	if(address(positionMgr) != address(0)){
	    uint256 idsLength = 0;
	    uint256[1024] memory ids;
	    uint256 ulen = positionMgr.userListLength();
	    for(uint256 n=0; n<ulen; ++n){
		address user = positionMgr.userList(n);
		uint256 numTokens = positionMgr.numberOfTokensOfOwner(user);
		for(uint256 m=0; m<numTokens; ++m){
		    uint256 id = positionMgr.tokenOfOwnerByIndex(user, m);
		    if (triggered(id))
			ids[idsLength++] = id;
		}
	    }
	    _ids = new uint256[](idsLength);
	    for(uint256 n=0; n<idsLength; ++n)
		_ids[n] = ids[n];
	    return _ids;
	}
    }
    function performUp(uint256[] memory _ids) public {
	lastTimestamp = block.timestamp;
	for(uint256 n=0; n<_ids.length; ++n)
	    if((LEVEL_TRIGGER && triggered(_ids[n])) || positionMgr.isOpen(_ids[n]))
		positionMgr.fillPos(_ids[n]);
    }
    function checkUpkeep() view public
	returns (bool upkeepNeeded,
		 bytes memory performData) {
	if(active)
	    if(block.timestamp - lastTimestamp > interval) {
		uint256[] memory _ids = checkUp();
		if(_ids.length > 0)
		    return(true, abi.encode(_ids));
	    }
    }
    function checkUpkeep(bytes calldata) view override external
	returns (bool upkeepNeeded,
		 bytes memory performData) {
	(upkeepNeeded, performData) = checkUpkeep();
    }
    function performUpkeep(bytes calldata performData) external override {
	lastTimestamp = block.timestamp;
	++count;
	performUp(abi.decode(performData, (uint256[])));
    }
    function testUpkeep() external {

	// save active mode
	bool isActive = active;
	// turn active mode on no matter what for now
	active = true;

	// run the read/trigger function
	(bool upkeepNeeded,
	 bytes memory performData) = checkUpkeep();

	// run the write function to we need to.
	if(upkeepNeeded)
	    this.performUpkeep(performData);
	
	// set active mode back to what it was
	active = isActive;
    }
}
