	int24    _tickUpper  = _tickLower   + _tickSpacing;
	uint128 _liquidity = sqrtPriceX96(_pool).getLiquidityForAmounts
	    (_tickLower.getSqrtRatioAtTick(), _tickUpper.getSqrtRatioAtTick(),
