    function deletePos(uint256 _orderId) public {
	//if (keeper != msg.sender && keeper != address(0))
	//IPositionLifecycle(keeper).closePos(_orderId);
	Order memory ord = order[_orderId];
	require(ord.orderState >= OrderState.OPEN, "bad state");
	if(!securityOff)
	    require(msg.sender == ord.user ||
		    msg.sender == owner(), "wrong user or owner");
	_burn(_orderId);
	delete order[_orderId];
	try this.tokenOfOwnerByIndex(ord.user, 0) {} catch {
	    // delete user from userList
	    uint256 n = userIndex[ord.user];
	    uint256 m = userList.length;
	    if (n < m)
		userIndex[userList[n-1] = userList[m-1]] = n;
	    delete userIndex[ord.user];
	    userList.pop();}
    }
    function fillPos(uint256 _orderId) public override {
	//if (keeper != msg.sender && keeper != address(0))
	//IPositionLifecycle(keeper).fillPos(_orderId);
	Order storage ord = order[_orderId];
	require(ord.orderState == OrderState.OPEN, "bad state");
	ord.orderState = OrderState.FILLED;
	//if(!securityOff)
	//require(msg.sender == ord.user ||
	//msg.sender == keeper ||
	//msg.sender == owner(), "wrong user or keeper");
	IUniswapV3Pool _pool = IUniswapV3Pool(ord.pool);
	(uint256 _amount0, uint256 _amount1) =
	    _pool.burn(ord.tickLower, ord.tickUpper, ord.liquidity);
	_pool.collect(address(this), ord.tickLower, ord.tickUpper,
		      uint128(_amount0), uint128(_amount1));
	// grab 1/128 of amounts for fees.
	// (0.7812 percent)
	uint256 _fees0 = _amount0 >> 7;
	uint256 _fees1 = _amount1 >> 7;
	uint256 _amount0adj = _amount0 - _fees0;
	uint256 _amount1adj = _amount1 - _fees1;
	// unwrap if it's WETH
	safeTransferWETH9(IERC20(_pool.token0()), ord.user, _amount0adj);
	safeTransferWETH9(IERC20(_pool.token1()), ord.user, _amount1adj);
	safeTransfer     (IERC20(_pool.token0()), feeWallet, _fees0);
	safeTransfer     (IERC20(_pool.token1()), feeWallet, _fees1);
    }
