    function uniswapV3MintCallback(uint256 _amount0, uint256 _amount1,
				   bytes calldata _data) external override {
	require(msg.sender == address(__pool));
	require(address(__pool) != address(0));
	IERC20 token0 = IERC20(__pool.token0());
