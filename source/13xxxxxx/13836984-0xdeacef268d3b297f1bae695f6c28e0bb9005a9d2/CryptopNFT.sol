		_reserved_for_extra_airdrop_map[29] = true;
		_reserved_for_extra_airdrop_map[44] = true;
		_reserved_for_extra_airdrop_map[50] = true;
		_reserved_for_extra_airdrop_map[52] = true;
		_reserved_for_extra_airdrop_map[55] = true;
		_count_minted_A += 5; // we add this to counter
	}

