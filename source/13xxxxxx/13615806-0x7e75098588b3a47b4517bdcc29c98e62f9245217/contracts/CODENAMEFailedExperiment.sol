pragma solidity ^0.8.0;

contract CODENAMEFailedExperiment {
    function burn(address user, uint256 amount) external pure {
        require(user == address(0) && amount == 0, "CODENAME: FAILED EXPERIMENT - FAILED TO BURN");
        revert("CODENAME: FAILED EXPERIMENT - FAILED TO BURN");
    }

    function updateReward(address from, address to) external pure {
        require(from != to, "CODENAME: FAILED EXPERIMENT");
    }
}
