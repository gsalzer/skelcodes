        bytes32 salt = "308fd119edae5f1309aafade64147";
        uint256 ran1 = random(salt[_saltI], _pepper, 100);
        uint8 traitIndex;
        uint8 i;
        while (_weights[i] < ran1) {
            traitIndex += _split[i];
            i++;
        }
        uint256 ran2 = random(salt[_saltI + 2], _pepper, _split[i]);
        return uint8(traitIndex + ran2);
