        uint256 tokenNumber = currentMintInfo.mintCount + 1;

        if (bytes(_dateCreated).length > 0) {
            currentMintInfo.dateCreated = _dateCreated;
        }

        if (bytes(_eipDescription).length > 0) {
            currentMintInfo.eipDescription = _eipDescription;
        }

